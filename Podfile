# Uncomment the next line to define a global platform for your project
 platform :ios, '10.0'

target 'ATNT' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for ATNT
  pod 'ObjectMapper', '~> 2.2'
    pod 'Kingfisher'
    pod 'Alamofire'
    pod 'SwiftyJSON'
    pod 'GoogleMaps'
    pod 'GooglePlaces'
    pod 'FontAwesome.swift'
    pod 'SVProgressHUD'
    pod 'PKHUD', '~> 4.0'
    
  target 'ATNTTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ATNTUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
